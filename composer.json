{
    "name": "joeymckenzie/typestream",
    "description": "Jetsream-based scaffolding for the Laravel framework including TypeScript for Inertia.",
    "keywords": ["laravel", "tailwind", "auth", "inertia"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/joeymckenzie/typestream/issues",
        "source": "https://github.com/joeymckenzie/typestream"
    },
    "authors": [
        {
            "name": "Joey McKenzie",
            "email": "joey.mckenzie.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "ext-json": "*",
        "illuminate/console": "^11.0",
        "illuminate/support": "^11.0",
        "laravel/fortify": "^1.20",
        "mobiledetect/mobiledetectlib": "^4.8",
        "symfony/console": "^7.0"
    },
    "require-dev": {
        "inertiajs/inertia-laravel": "^1.0",
        "laravel/sanctum": "^4.0",
        "livewire/livewire": "^3.3",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Jetstream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Jetstream\\Tests\\": "tests/",
            "App\\": "stubs/app/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Jetstream\\JetstreamServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
